{
    "coc.preferences.formatOnSaveFiletypes": [
        "css",
        "javascript",
        "graphql",
        "html",
        "yaml",
        "json",
        "python",
        "xml"
    ],
    "suggest.completionItemKindLabels": {
        "method": "  ",
        "function": "  ",
        "variable": "[]",
        "field": "  ",
        "typeParameter": "<>",
        "constant": "  ",
        "class": " פּ ",
        "interface": " 蘒",
        "struct": "  ",
        "event": "  ",
        "operator": "  ",
        "module": "  ",
        "property": "  ",
        "enum": " 練",
        "reference": "  ",
        "keyword": "  ",
        "file": "  ",
        "folder": " ﱮ ",
        "color": "  ",
        "unit": " 塞 ",
        "snippet": "  ",
        "text": "  ",
        "constructor": "  ",
        "value": "  ",
        "enumMember": "  "
    },
    "python.analysis.logLevel": "Warning",
    "python.pythonPath": "python3",
    "python.venvPath": "~/.conda/env/",
    "python.formatting.provider": "autopep8",
    "pyright.disableDiagnostics": true,
    "diagnostic-languageserver.filetypes": {
        "python": [
            "flake8",
            "mypy"
        ],
        "javascript": "eslint"
    },
    "diagnostic-languageserver.formatFiletypes": {
        "python": "black",
        "javascript": "prettier"
    },
    "diagnostic-languageserver.formatters": {
        "black": {
            "command": "black",
            "args": [
                "--quiet",
                "--line-length",
                "79",
                "-"
            ]
        }
    },
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "all",
    "emmet.showAbbreviationSuggestions": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "prettier.disableLanguages": [
        "yaml",
        "json"
    ],
    "explorer.width": 35,
    "explorer.buffer.root.template": "[icon & 1] BUFFERS",
    "explorer.file.root.template": "[icon & 1] TREE ([root])",
    "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
    "explorer.file.showHiddenFiles": true,
    "explorer.file.reveal.auto": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.icon.source": "vim-devicons",
    "explorer.previewAction.onHover": false,
    "explorer.git.showIgnored": true,
    "explorer.keyMappings.global": {
        "i": "open:vsplit:intelligent",
        "o": "open:split:intelligent",
        "e": ""
    },
    "yaml.format.enable": true
}
